  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: a8a1b1f86712ea244943dd893f9d9b1a, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  dialogue: 
  dialogueLines:
  - "Bed\xFCrftiger 2: Gr\xFC\xDF Gott, Herr Heinrich! Ich habe geh\xF6rt, dass Sie
    Ihren Besitz verschenken."
  - "Heinrich: Du hast richtig geh\xF6rt. Auch du wirst etwas davon bekommen. Kennst
    du oder jemand im Dorf eine Heilung gegen Aussatz?"
  - "Bed\xFCrftiger 2: Leider nein, gewiss kenne ich aber jemanden, der Ihnen helfen
    k\xF6nnte. Gehen Sie weiter ins Dorf und fragen Sie nach dem Meier."
  - "Curt: Wird er meinem Cousin helfen k\xF6nnen?"
  - "Bed\xFCrftiger 2: Das wei\xDF ich nicht. Es gibt eine alte Sage \xFCber Aussatz.
    Auch damals hatte ein wohlhabender und angesehener Mann wie Sie Aussatz."
  - 'Heinrich: Wie wurde ihm denn geholfen?'
  - "Bed\xFCrftiger 2: Das Ende der Sage wird von jedem anders erz\xE4hlt. Am besten
    gehen Sie zum Meier, mein Herr, er kennt gewiss das Ende."
  idSzene: 10
