MonoBehaviour:
  m_ObjectHideFlags: 0
  m_PrefabParentObject: {fileID: 0}
  m_PrefabInternal: {fileID: 0}
  m_GameObject: {fileID: 1855160966}
  m_Enabled: 1
  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: a8a1b1f86712ea244943dd893f9d9b1a, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  dialogue: 
  dialogueLines:
  - "Agnes: Herr Heinrich, wie f\xFChlen Sie sich?"
  - "Heinrich: Es wird immer schlimmer. Fr\xFCher dachte ich, der Verlust meines Besitzes
    wird mich hart treffen. Aber die Krankheit ist viel schlimmer."
  - "Agnes: Ich w\xFCrde Ihnen gerne helfen. Aber ich wei\xDF nicht, wie."
  - "Heinrich: Im Dorf erz\xE4hlte man mir, dein Vater kenne das Ende der Sage."
  - "Agnes: Das w\xE4re mir neu."
  - "Heinrich: Lass uns aber nicht mehr \xFCber mich sprechen. Ich habe dir als Dank,
    dass dein Vater mich bei euch beh\xE4lt, Geschenke mitgebracht."
  - "Agnes: Aber Herr Heinrich, Sie haben doch kaum noch etwas. Das m\xFCssen Sie
    doch nicht f\xFCr mich ausgeben!"
  - 'Heinrich: Ich werde auf ewig in eurer Schuld stehen. Komm mit, dann kannst du
    dir deine Geschenke suchen.'
  idSzene: 13
