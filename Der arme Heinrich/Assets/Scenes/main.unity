  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: a8a1b1f86712ea244943dd893f9d9b1a, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  dialogue: 
  dialogueLines:
  - 'Heinrich: Ich hoffe, dir ist bewusst geworden, wie sehr du mir am Herzen liegst.'
  - "Agnes: Du mir auch. Ich m\xF6chte den Rest meiner Zeit mit dir verbringen."
  - "Heinrich: Oh, Agnes. Das kann ich nur zur\xFCckgeben. Aber solange es keine Heilung
    f\xFCr mich gibt, wird die gemeinsame Zeit sehr kurz sein."
  - "Agnes: Ich w\xFCrde dir gerne helfen. Aber auch ich wei\xDF das Ende der Sage
    nicht. Es blieb mir immer verschwiegen."
  idSzene: 14
