  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: a8a1b1f86712ea244943dd893f9d9b1a, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  dialogue: 
  dialogueLines:
  - 'Curt: Cousin, was hast du nun vor?'
  - "Heinrich: Ich bin durch die Bestrafung einer unheilbaren Krankheit wertlos. Was
    mich neben meiner Pers\xF6nlichkeit am meisten ausgezeichnet hat, war mein Besitz."
  - 'Curt: Warum schauen uns die Menschen denn so komisch an?'
  - 'Heinrich: Sieh mich doch an. Ich ekel mich doch selbst.'
  - "Curt: Aber was k\xF6nnen wir denn dagegen tun, Cousin?"
  - 'Heinrich: Solange es mir noch gut geht, werde ich nun meinen Besitz verteilen.'
  - 'Curt: Aber an wen? Niemand kann sich so etwas leisten!'
  - "Heinrich: Denke nicht so von mir. Ich werde meinen Besitz gerecht an die Bed\xFCrftigen
    verschenken."
  idSzene: 8
