  m_Enabled: 1
  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: a8a1b1f86712ea244943dd893f9d9b1a, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  dialogue: 
  dialogueLines:
  - "Heinrich: Ich wei\xDF nicht, wie ich eine so selbstlose Jungfrau finden soll."
  - "Agnes: Ich kann mich f\xFCr dich opfern."
  - 'Mutter: Das kommt auf gar keinen Fall in Frage. Du handelst nicht selbstlos,
    sondern aus Liebe! Das ist nur Mitleid!'
  - "Agnes: Mutter, ich habe schon lange \xFCberlegt, wie ich meinem Heinrich helfen
    kann. Das w\xE4re die optimalste L\xF6sung."
  - "Mutter: Und was erhoffst du dir dadurch? Wie k\xF6nnten wir nur ohne dich weiterleben?"
  - "Agnes: Mutter, ihr h\xE4ttet so noch das Grundst\xFCck, auf dem ihr lebt, speist
    und trinkt. Auf mich k\xF6nnt ihr auch verzichten. F\xFCr mich steht die Entscheidung
    fest."
  idSzene: 16
