  m_Script: {fileID: 11500000, guid: a8a1b1f86712ea244943dd893f9d9b1a, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  dialogue: 
  dialogueLines:
  - "Bed\xFCrftiger 1: Herr Heinrich! Welche Ehre, dass Sie bei mir Halt machen. Wie
    komme ich zu dieser Ehre?"
  - "Heinrich: Ich m\xF6chte meinen Besitz an die Bed\xFCrftigen verteilen. Ich wurde
    mit Aussatz bestraft und bin nicht zuversichtlich, dass es eine Heilung gibt."
  - "Bed\xFCrftiger 1: Mein Herr, eine Heilung kenne ich nicht. \xDCber den Besitz
    freue ich mich allerdings. Gehen Sie weiter ins Dorf, eventuell kann Ihnen jemand
    helfen."
  - 'Heinrich: Gott sei mit dir. Ich werde voranschreiten.'
  idSzene: 9
